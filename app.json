{
  "expo": {
    "name": "Kyte",
    "slug": "Kyte",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/kyte-icon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./images/kyte-logo-green.png",
      "resizeMode": "contain",
      "backgroundColor": "#232230"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/kyte-logo-green.png",
        "backgroundColor": "#232230"
      }
    },
    "web": {
      "favicon": "./images/favicon.ico"
    }
  }
}